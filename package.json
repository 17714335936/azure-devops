{
	"name": "azure-devops",
	"version": "0.0.1",
	"description": "A Demo of Azure DevOps CI/CD",
	"repository": "https://github.com/jcailan/azure-devops",
	"license": "MIT",
	"dependencies": {
		"@sap/cds": "^3",
		"express": "^4"
	},
	"scripts": {
		"start": "npx cds run"
	},
	"cds": {
		"requires": {
			"NorthWind": {
				"kind": "odata",
				"model": "srv/external/NorthWind",
				"credentials": {
					"destination": "demo-destination",
					"prefix": "Experimental/OData/OData.svc"
				}
			}
		}
	},
	"devDependencies": {
		"sqlite3": "^4.2.0"
	}
}